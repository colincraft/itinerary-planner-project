<h1>Itineraries#edit</h1>

<%= form_for @itinerary do |f| %>

<%= f.label :name_i %>
<%= f.text_field :name_i%>

<%= f.label :origin %>
<%= f.text_field :origin%>

<%= f.label :city %>
<%= f.text_field :city%>

<%= f.label :departure_date %>
<%= f.text_field :departure_date%>

<%= f.label :return_date %>
<%= f.text_field :return_date%>

<%= f.submit "submit" %>
<% end %>
